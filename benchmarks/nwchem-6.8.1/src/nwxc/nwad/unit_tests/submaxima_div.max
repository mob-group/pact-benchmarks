fx(x):=cos(x)/exp(x);
dfx(x):=diff(fx(x),x);
df2x(x):=diff(dfx(x),x);
df3x(x):=diff(df2x(x),x);
Fortran(expr):= fortran(ev(expr,numer));
with_stdout("submaxima_div.F",
  print("      subroutine submaxima_div(x,f,df,df2,df3)"),
  print("      implicit none"),
  print("      double precision x,f,df,df2,df3"),
  Fortran(f = fx(x)),
  Fortran(df = dfx(x)),
  Fortran(df2 = df2x(x)),
  Fortran(df3 = df3x(x)),
  print("      end"));
